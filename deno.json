{
  "name": "@elsoul/fresh-theme",
  "version": "1.3.0",
  "description": "Theme Module for Fresh v2 App.",
  "runtimes": ["deno", "browser"],
  "exports": "./mod.ts",
  "tasks": {
    "dev": "deno test --watch mod.ts",
    "test": "deno test --watch mod_test.ts"
  },
  "license": "Apache-2.0",
  "imports": {
    "@preact/signals": "npm:@preact/signals@1.3.0",
    "preact/hooks": "npm:preact@10.24.3/hooks",
    "fresh-atom": "jsr:@elsoul/fresh-atom@1.0.0"
  },
  "fmt": {
    "options": {
      "semiColons": false,
      "singleQuote": true
    }
  },
  "compilerOptions": {
    "lib": ["dom", "deno.ns"]
  }
}
